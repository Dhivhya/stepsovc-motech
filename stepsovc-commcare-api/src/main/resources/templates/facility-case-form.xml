<?xml version='1.0' ?>
<data uiVersion="1" version="40" name="Facility Registration"
      xmlns="http://openrosa.org/formdesigner/8F9255C2-3A1D-4B82-8346-561264CEBEA0">
    <facility_code>$!facility.facilityCode</facility_code>
    <facility_name>$!facility.facilityName</facility_name>
    <case>
        <case_id>$!facility.id</case_id>
        <create>
            <case_type_id>facility</case_type_id>
            <case_name>$!facility.facilityCode</case_name>
            <user_id>$!facility.id</user_id>
            <external_id>$!facility.facilityCode</external_id>
        </create>
        <update>
            <facility_name>$!facility.facilityName</facility_name>
            <facility_code>$!facility.facilityCode</facility_code>
        </update>
    </case>
    <meta>
        <username>allUsers</username>
        <userID>$!facility.id</userID>
    </meta>
</data>

