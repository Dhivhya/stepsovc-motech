<Registration xmlns="http://openrosa.org/user/registration">
    <username>$caregiver.userName</username>
    <password>1234</password>
    <uuid>$caregiver.caregiverId</uuid>
    <date>$caregiver.creationDate</date>
    <registering_phone_id>$!caregiver.phoneNumber</registering_phone_id>
    <user_data>
        <data key="caregiverCode">$caregiver.caregiverCode</data>
        <data key="firstName">$caregiver.firstName</data>
        <data key="middleName">$caregiver.middleName</data>
        <data key="lastName">$caregiver.lastName</data>
        <data key="gender">$caregiver.gender</data>
    </user_data>
</Registration>